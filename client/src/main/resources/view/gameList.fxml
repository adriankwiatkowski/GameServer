<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="500.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.client.controller.GameListController">
    <children>
        <HBox prefHeight="500.0" prefWidth="850.0">
            <children>
                <VBox prefHeight="500.0" prefWidth="514.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#backToMenu" prefHeight="25.0" prefWidth="500.0"
                                text="BACK"/>
                        <Pagination fx:id="pagination" minHeight="400.0" prefHeight="479.0" prefWidth="500.0"/>
                    </children>
                </VBox>
                <VBox fx:id="gameInfo" maxWidth="350.0" minHeight="500.0" minWidth="278.0" prefHeight="500.0"
                      prefWidth="350.0"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
